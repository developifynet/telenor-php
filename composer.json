{
    "name": "developifynet/telenor-php",
    "description": "Telenor Pakistan Corporate SMS API Wrapper for PHP",
    "license": "MIT",
    "keywords": [
        "telenor",
        "corporate sms",
        "api",
        "php",
        "sdk"
    ],
    "authors": [
        {
            "name": "Mustafa Mughal",
            "email": "efficientbit@gmail.com"
        },
        {
            "name": "Mudassar Mughal",
            "email": "mudassary0@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4|>=8.0|>=8.1",
        "illuminate/support": "~6.0|~7.0|~8.0|~9.0",
        "guzzlehttp/guzzle": "~6.0|~7.0|~8.0|~9.0"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": "^6.1|^7.1|^8.1|^9.1"
    },
    "autoload": {
        "psr-0": {
            "Developifynet\\Telenor": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Developifynet\\Telenor\\TelenorServiceProvider"
            ],
            "aliases": {
                "Telenor": "Developifynet\\Telenor\\Telenor"
            }
        }
    }
}
